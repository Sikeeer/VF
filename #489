#include <stdio.h>

int main() {
    int n;
    int min = 1000000; 
    int max = 0;   

    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        int element;
        scanf("%d", &element);

        if (element < min) {
            min = element; }

        if (element > max) {
            max = element;}
    }
    printf("%d %d\n", min, max);
    return 0;
}
